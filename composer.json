{
    "name": "dz0ny/heroku-docker-piwik",
    "repositories": [{
        "type": "package",
        "package": {
            "name": "piwik",
            "version": "2.16.1",
            "type": "project",
            "dist": {
                "type": "zip",
                "url": "https://builds.piwik.org/piwik-2.16.1.zip"
            }
        }
    }],
    "authors": [{
        "name": "Janez Troha",
        "email": "dz0ny@ubuntu.si"
    }],
    "require": {
        "php": "*",
        "piwik": "*",
        "ext-mbstring": "*",
        "ext-gd": "*",
        "ext-apcu": "*",
        "geoip/geoip": "*"
    },
    "require-dev": {
        "heroku/heroku-buildpack-php": "*"
    },
    "scripts": {
        "compile": [
            "bash scripts/compile.sh"
        ]
    }
}
